<global loop="true" default_path="samples/nylonguitar">
  <group attack="0.01" release="0.1" >
    <region sample="guit #2 e2.flac" lokey="36" hikey="42" keycenter="52"/>
    <region sample="guit #2 e2.flac" lokey="43" hikey="52" keycenter="52"/>
    <region sample="guit #2 g2.flac" tune_offset="9" lokey="53" hikey="54" keycenter="55"/>
    <region sample="guit #2 g2.flac" lokey="55" hikey="55" keycenter="55"/>
    <region sample="guit #2 a2.flac" lokey="56" hikey="56" keycenter="57"/>
    <region sample="guit #2 a2.flac" lokey="57" hikey="58" keycenter="57"/>
    <region sample="guit #2 c3.flac" lokey="59" hikey="60" keycenter="60"/>
    <region sample="guit #2 d3.flac" lokey="61" hikey="61" keycenter="62"/>
    <region sample="guit #2 d3.flac" lokey="62" hikey="63" keycenter="62"/>
    <region sample="guit #2 f3.flac" lokey="64" hikey="65" keycenter="65"/>
    <region sample="guit #2 g3.flac" lokey="66" hikey="66" keycenter="67"/>
    <region sample="guit #2 g3.flac" lokey="67" hikey="67" keycenter="67"/>
    <region sample="guit #2 a3.flac" lokey="68" hikey="68" keycenter="69"/>
    <region sample="guit #2 a3.flac" lokey="69" hikey="70" keycenter="69"/>
    <region sample="guit #2 b3.flac" lokey="71" hikey="73" keycenter="71"/>
    <region sample="guit #2 e4.flac" lokey="74" hikey="78" keycenter="76"/>
    <region sample="guit #2 a4.flac" lokey="79" hikey="82" keycenter="81"/>
    <region sample="guit #2 b4.flac" lokey="83" hikey="85" keycenter="83"/>
    <region sample="guit #2 d4.flac" lokey="86" hikey="87" keycenter="86"/>
    <region sample="guit #2 d4.flac" lokey="88" hikey="108" keycenter="86"/>
    <region sample="guit #1 e2.flac" lokey="36" hikey="53" keycenter="52"/>
    <region sample="guit #1 g2.flac" lokey="54" hikey="54" keycenter="55"/>
    <region sample="guit #1 g2.flac" lokey="55" hikey="56" keycenter="55"/>
    <region sample="guit #1 a2.flac" lokey="57" hikey="59" keycenter="57"/>
    <region sample="guit #1 f3.flac" lokey="65" hikey="65" keycenter="65"/>
    <region sample="guit #1 d4.flac" lokey="74" hikey="74" keycenter="74"/>
    <region sample="guit #1 c3.flac" lokey="60" hikey="61" keycenter="60"/>
    <region sample="guit #1 d3.flac" lokey="62" hikey="63" keycenter="62"/>
    <region sample="guit #1 d3.flac" lokey="64" hikey="64" keycenter="62"/>
    <region sample="guit #1 g3.flac" lokey="66" hikey="66" keycenter="67"/>
    <region sample="guit #1 g3.flac" lokey="67" hikey="68" keycenter="67"/>
    <region sample="guit #1 a3.flac" lokey="69" hikey="70" keycenter="69"/>
    <region sample="guit #1 b3.flac" lokey="71" hikey="71" keycenter="71"/>
    <region sample="guit #1 b3.flac" lokey="72" hikey="73" keycenter="71"/>
    <region sample="guit #1 e4.flac" lokey="75" hikey="75" keycenter="76"/>
    <region sample="guit #1 e4.flac" lokey="76" hikey="76" keycenter="76"/>
    <region sample="guit #1 g4.flac" lokey="77" hikey="79" keycenter="79"/>
    <region sample="guit #1 a4.flac" lokey="80" hikey="80" keycenter="81"/>
    <region sample="guit #1 a4.flac" lokey="81" hikey="120" keycenter="81"/>
  </group>

  <!--Strum groups-->
  <!-- A4 or B4 on 60-->
  <group chan="1" key="60" one_shot="true">
    <region sample="guit #2 a3.flac" maxProb="0.5" />
    <region sample="guit #1 a3.flac" maxProb="0.5" />
    <region sample="guit #2 b3.flac" minProb="0.5" />
    <region sample="guit #1 b3.flac" minProb="0.5" />
  </group>
  <!-- C#5 or D5 on 61-->
  <group chan="1" key="61" one_shot="true" delay="0.0675">
    <region sample="guit #2 b3.flac" note_offset="2"  maxProb="0.5" />
    <region sample="guit #1 b3.flac" note_offset="2"  maxProb="0.5" />
    <region sample="guit #2 e4.flac" note_offset="-2" minProb="0.5" />
    <region sample="guit #1 d4.flac"                  minProb="0.5" />
  </group>
  <!-- E5 or F#5 on 62-->
  <group chan="1" key="62" one_shot="true" delay="0.0675">
    <region sample="guit #2 e4.flac" maxProb="0.5" />
    <region sample="guit #1 e4.flac" maxProb="0.5" />
    <region sample="guit #2 e4.flac" note_offset="2" minProb="0.5" />
    <region sample="guit #1 g4.flac" note_offset="-1" minProb="0.5" />
  </group>
  <!-- E5 or F#5 on 63-->
  <group chan="1" key="63" one_shot="true" delay="0.125">
    <region sample="guit #2 e4.flac" maxProb="0.5" />
    <region sample="guit #1 e4.flac" maxProb="0.5" />
    <region sample="guit #2 e4.flac" note_offset="2" minProb="0.5" />
    <region sample="guit #1 g4.flac" note_offset="-1" minProb="0.5" />
  </group>
  <!-- A5 or B5 on 64-->
  <group chan="1" key="64" one_shot="true" delay="0.125">
    <region sample="guit #2 a4.flac" maxProb="0.5" />
    <region sample="guit #1 a4.flac" maxProb="0.5" />
    <region sample="guit #2 b4.flac" minProb="0.5" />
    <region sample="guit #1 a4.flac" note_offset="2" minProb="0.5" />
  </group>

  <effect type="filter" cutoff="420"/>
  <effect type="reverb" space="hall_large" wet="1" dry="1" damp="0" size="0.5" tone="2560"/>
</global>
